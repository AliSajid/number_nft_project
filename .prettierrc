{
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-multiline-arrays"
    ],
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
}
